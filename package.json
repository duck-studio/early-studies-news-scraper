{
  "name": "hono-serper-search",
  "version": "1.0.0",
  "description": "Hono API for concurrent news article fetching using Serper API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev src/index.ts --local",
    "deploy": "wrangler deploy src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "types": "npx wrangler types",
    "check": "biome check --apply .",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate-types": "wrangler types > src/types/cloudflare.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/zod-validator": "^0.4.3",
    "@scalar/hono-api-reference": "^0.7.4",
    "async-retry": "^1.3.3",
    "date-fns": "^4.1.0",
    "hono": "^4.4.12",
    "hono-openapi": "^0.4.6",
    "nanoid": "^5.0.6",
    "p-limit": "^4.0.0",
    "pino": "^9.2.0",
    "zod": "^3.24.2",
    "zod-openapi": "^4.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/async-retry": "^1.4.8",
    "@types/node": "^20.17.28",
    "eslint": "^8.x",
    "pino-pretty": "^11.3.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.2",
    "vitest": "^1.4.0",
    "wrangler": "^4.4.0"
  }
}