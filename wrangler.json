{
  "name": "early-studies-api",
  "main": "src/index.ts",
  "compatibility_date": "2024-01-01",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "routes": [
    {
      "pattern": "api.earlystudies.com",
      "custom_domain": true
    },
    {
      "pattern": "early-studies-api.early-studies-api.workers.dev",
      "custom_domain": false
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "early-studies-d1-database",
      "database_id": "770fd9ff-f7fa-40bc-9a83-b6036ddff67e",
      "migrations_dir": "src/db/migrations"
    }
  ],
  "workflows": [
    {
      "name": "ProcessNewsItemWorkflow",
      "binding": "PROCESS_NEWS_ITEM_WORKFLOW",
      "class_name": "ProcessNewsItemWorkflow"
    }
  ],
  "triggers": {
    "crons": ["0 0 * * *"]
  }
} 